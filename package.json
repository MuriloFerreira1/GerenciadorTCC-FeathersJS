{
  "name": "gerenciador-tcc-feathers-js",
  "description": "Servidor da aplicação de gerenciamento de TCC utilizando api REST e conexão com o banco de dados por meio da biblioteca Sequelize",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Murilo Ferreira",
    "email": "rillofelix@hotmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^20.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run compile && npm run jest",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "dev": "ts-node-dev --no-notify src/",
    "start": "npm run compile && node lib/",
    "jest": "jest --forceExit",
    "compile": "shx rm -rf lib/ && tsc"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/authentication": "^4.5.18",
    "@feathersjs/authentication-local": "^4.5.18",
    "@feathersjs/authentication-oauth": "^4.5.18",
    "@feathersjs/configuration": "^4.5.17",
    "@feathersjs/errors": "^4.5.17",
    "@feathersjs/express": "^4.5.18",
    "@feathersjs/feathers": "^4.5.17",
    "@feathersjs/hooks": "^0.9.0",
    "@feathersjs/socketio": "^4.5.18",
    "@feathersjs/transport-commons": "^5.0.27",
    "@types/node": "^20.14.11",
    "@types/nodemailer": "^6.4.15",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-authentication-management": "4.0",
    "feathers-hooks-common": "^6.1.5",
    "feathers-mailer": "^4.1.1",
    "feathers-sequelize": "^6.4.0",
    "helmet": "^5.1.1",
    "mysql2": "^3.10.1",
    "nodemailer": "^6.9.14",
    "sequelize": "^6.37.3",
    "serve-favicon": "^2.5.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.42",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/serve-favicon": "^2.5.7",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "axios": "^1.6.8",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.2.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
